{
    "os": "ubuntu:latest",
    "dockerfile": "./Dockerfile",
    "buildargs": {
        "ARG1": "-d .",
        "ARG2": ""
    },
    "tag": "my-image:latest",
    "name": "my-container",
    "command": "/bin/bash",
    "path": ".",
    "ports": {
        "80/tcp": 8080,
        "443/tcp": 8443
    },
    "environment": {
        "ENV_VAR1": "value1",
        "ENV_VAR2": "value2"
    },
    "volumes": {
        "/host/path": {
            "bind": "/container/path",
            "mode": "rw"
        }
    },
    "packages": [
        "curl",
        "wget",
        "nano"
    ],
    "libraries": [
        "numpy",
        "pandas",
        "matplotlib"
    ]
}
